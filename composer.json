{
  "name": "pocket/djob",
  "description": "A database job library",
  "keywords": [
    "php database job",
    "php database queue"
  ],
  "license": "MIT",
  "repositories": [
    {
      "type": "path",
      "url": "../*"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-ctype": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "laravel/serializable-closure": "^1.3",
    "workerman/workerman": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Pocket\\Djob\\": "src/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
